{
    "plugins": [
        [
            "relay",
            {
                "artifactDirectory": "./__generated__/relay/"
            }
        ],
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
        ["@babel/preset-react", {"runtime": "automatic"}],
        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "env": {
        "production": {
            "compact": true,
            "comments": false,
            "minified": true
        }
    },
    "sourceMaps": true
}